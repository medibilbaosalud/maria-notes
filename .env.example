# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Groq API Key (recommended for final fallback + async follow-up validation)
VITE_GROQ_API_KEY=your_groq_api_key_here

# Gemini API key (required for primary single-shot clinical pipeline)
VITE_GEMINI_API_KEY=your_gemini_api_key_here
# Optional: multiple Gemini keys (comma-separated)
VITE_GEMINI_API_KEYS=

# Optional router overrides
VITE_AI_ALLOWED_MODELS_JSON=
VITE_AI_TASK_MODEL_OVERRIDES_JSON=
VITE_AI_MODEL_LIMITS_JSON=

# Fast path latency tuning
VITE_FAST_PATH_ADAPTIVE_VALIDATION=true
VITE_FAST_PATH_TOKEN_BUDGETS=true
VITE_FAST_PATH_RETRY_TUNING=true
VITE_FAST_PATH_ASYNC_TRIAGE=true
VITE_TWO_CALL_ADAPTIVE_MODE=true
VITE_SINGLE_SHOT_HISTORY_MODE=true

# Turbo v5 profile tuning
# aggressive_p95: max speed, more parallelism/hedging
# conservative_cost: lower cost, safer limits
VITE_TURBO_PROFILE=aggressive_p95
VITE_PIPELINE_V5_TURBO=true
VITE_TURBO_FAST_DRAFT=true
VITE_TURBO_HARDENING_ASYNC=true
VITE_TURBO_HEDGED_REQUESTS=true
VITE_GEMINI_ONE_CALL_STRICT=true
VITE_SINGLE_SHOT_ASYNC_FOLLOWUP=false

# Optional explicit overrides (leave empty to use profile defaults)
# VITE_TURBO_GLOBAL_CONCURRENCY_LIMIT=8
# VITE_TURBO_ROUTE_CONCURRENCY_LIMIT=2
# VITE_TURBO_CIRCUIT_COOLDOWN_MS=6000
# VITE_STT_HEDGE_TRIGGER_MS=2000
# VITE_STT_MIN_CONCURRENCY=1
# VITE_STT_MAX_CONCURRENCY=4
# VITE_STT_DOWN_THRESHOLD_MS=15000
# VITE_STT_UP_THRESHOLD_MS=6500
# VITE_STT_CHUNK_SLA_MS=20000
# VITE_TURBO_RECORDER_BATCH_INTERVAL_MS=600000
# VITE_AUDIO_PRECONVERSION_TIMEOUT_MS=12000
# VITE_PIPELINE_FINALIZE_WATCHDOG_MS=240000
# VITE_PIPELINE_CLOUD_MERGE_TIMEOUT_MS=15000
# VITE_RECORDER_ROTATE_STOP_START=true
# VITE_TURBO_DYNAMIC_ROUTING=true
# VITE_TURBO_LLM_HEDGING=true
# VITE_TURBO_LLM_HEDGE_TRIGGER_MS=2200
# VITE_TURBO_HARDENING_STALL_MS=180000
# VITE_TURBO_DRAFT_STALL_MS=240000
